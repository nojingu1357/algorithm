#include <iostream>
#include <deque>

using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    deque<int> d;

    int n,m,x,ave,sum=0,cnt=0;
    
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>x;
        d.push_back(x);
    }
    cin>>m;

    ave=m/n;
    for(int i=0;i<n;i++){
        if(d[i]<=ave){
            sum+=(ave-d[i]);
        } else {
            cnt++;
        }
    }
    
    cout<<ave+sum/cnt<<endl;
    return 0;
}
